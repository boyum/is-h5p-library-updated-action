{
  "name": "is-h5p-library-updated-action",
  "version": "0.0.0",
  "private": true,
  "description": "Checks the current branch's and the main branch's H5P library version to see if the current branch is ahead of the main branch",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format:fix": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint src/**/*.ts --fix",
    "lint-check": "eslint src/**/*.ts",
    "package": "bun run build && ncc build --source-map --license licenses.txt",
    "all": "bun run format:fix && bun run lint && bun run package && bun test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boyum/is-h5p-library-updated-action.git"
  },
  "keywords": [
    "actions",
    "h5p"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "bun-types": "^1.0.1"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.2",
    "@types/node": "^20.6.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vercel/ncc": "^0.38.0",
    "bun": "^1.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-github": "^4.10.0",
    "eslint-plugin-jest": "^27.2.3",
    "h5p-types": "^3.0.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.3",
    "prettier-config": "github:boyum/prettier-config",
    "typescript": "^5.2.2"
  },
  "prettier": "prettier-config"
}
