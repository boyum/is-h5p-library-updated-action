{
  "name": "is-h5p-library-updated-action",
  "version": "0.0.0",
  "private": true,
  "description": "Checks the current branch's and the main branch's H5P library version to see if the current branch is ahead of the main branch",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts --fix",
    "lint-check": "eslint src/**/*.ts",
    "package": "npm run build && ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "@typescript-eslint/parser": "^5.11.0",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.9.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^26.1.0",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
