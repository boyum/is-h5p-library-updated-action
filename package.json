{
  "name": "is-h5p-library-updated-action",
  "version": "0.0.0",
  "private": true,
  "description": "Checks the current branch's and the main branch's H5P library version to see if the current branch is ahead of the main branch",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts --fix",
    "lint-check": "eslint src/**/*.ts",
    "package": "npm run build && ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.1",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.4.5",
    "@typescript-eslint/parser": "^6.4.1",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.47.0",
    "eslint-plugin-github": "^4.9.2",
    "eslint-plugin-jest": "^27.2.3",
    "h5p-types": "^2.0.1",
    "jest": "^29.6.4",
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
